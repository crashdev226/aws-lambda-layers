{
    "name": "bref/bref-fpm",
    "license": "MIT",
    "type": "project",
    "description": "Bref is a framework to write and deploy serverless PHP applications on AWS Lambda.",
    "homepage": "https://bref.sh",
    "keywords": ["bref", "serverless", "aws", "lambda", "faas"],
    "repositories": [
        {
            "type": "path",
            "url": "/package",
            "options": {
                "symlink": false
            }
        }
    ],
    "require": {
        "php": ">=7.3.0",
        "ext-curl": "*",
        "ext-json": "*",
        "bref/bref": "dev-master",
        "psr/http-message": "^1.0",
        "hollodotme/fast-cgi-client": "^3.0",
        "riverline/multipart-parser": "^2.0.6",
        "psr/http-server-handler": "^1.0",
        "psr/container": "^1.0|^2.0"
    },
    "replace": {
        "mnapoli/silly": "*",
        "symfony/filesystem": "*",
        "async-aws/lambda": "*",
        "nyholm/psr7": "*",
        "symfony/yaml": "*",
        "symfony/http-client": "*"
    }
}
